    "plugin",
    "postgres",
    "postgrest",
    "vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/technowledgy/vue-postgrest.git"
  },
  "type": "module",
  "main": "dist/cjs/vue-postgrest.js",
  "exports": {
    ".": {
      "require": "dist/cjs/vue-postgrest.js",
      "default": "dist/es/vue-postgrest.js"
